algoritmo "Exercicio"
var
   Ano_atual, Ano_nascimento, Idade: inteiro
inicio
      Escreval("Em que ano nos estamos ?")
      Leia (Ano_atual)
      Escreval ("Em que ano você nasceu ?")
      Leia (Ano_nascimento)
      Idade <- Ano_atual-Ano_nascimento
      Escreva ("Você tem ", Idade, " anos de idade")


fimalgoritmo