Algoritmo "MeuNome"
Var
   nome: caractere
   numero1, numero2, S: inteiro
Inicio
   escreva ("Digite seu nome: ")
   Leia(nome)
   escreval ("Olá ", nome, " muito prazer.")
   escreva (nome, " digite um numero: ")
   Leia (numero1)
   escreva (nome, " digite outro numero: ")
   Leia (numero2)
   S <- numero1+numero2
   escreva ("A soma entre ", numero1," e ", numero2," é ",S)
Fimalgoritmo