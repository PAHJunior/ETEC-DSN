<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".localizar">
    <ImageButton
            android:src="@drawable/abc_ic_ab_back_material"
            android:layout_width="58"
            android:layout_height="58"
            android:id="@+id/imageButton2" android:contentDescription="@string/todo"
            tools:targetApi="donut"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="296dp" android:layout_marginRight="296dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/textView2" android:layout_marginTop="30dp"/>
    <EditText
            android:layout_width="246dp"
            android:layout_height="45dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/editText"
            android:textColor="@android:color/black" android:textSize="24sp" android:importantForAutofill="no"
            tools:targetApi="o"
            app:layout_constraintStart_toEndOf="@+id/textView2"
            android:layout_marginLeft="6dp" android:layout_marginStart="6dp" android:layout_marginTop="120dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="12dp" android:layout_marginRight="12dp" android:layout_marginBottom="483dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:text="@string/cpf"
            android:layout_width="64dp"
            android:layout_height="45dp"
            android:id="@+id/textView2"
            android:textSize="30sp" android:textStyle="bold"
            android:textColor="@android:color/black"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp" android:layout_marginTop="38dp"
            app:layout_constraintTop_toBottomOf="@+id/imageButton2" app:layout_constraintEnd_toStartOf="@+id/editText"
            android:layout_marginEnd="6dp" android:layout_marginRight="6dp" android:layout_marginBottom="483dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
</android.support.constraint.ConstraintLayout>